{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"format": {
			"outputs": [],
			"inputs": ["src/**", "external/**", ".prettierrc.json"]
		},
		"format:check": {
			"outputs": [],
			"inputs": ["src/**", "external/**", ".prettierrc.json"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"db:push": {
			"cache": false
		},
		"db:studio": {
			"cache": false,
			"persistent": true
		},
		"db:generate": {
			"cache": false
		},
		"db:sync": {
			"cache": false
		}
			}
}
